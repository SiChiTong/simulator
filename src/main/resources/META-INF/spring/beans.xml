<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-4.1.xsd">
	
	<context:component-scan base-package="com.xxxtai.simulator"/>

	<bean id="graph" class="com.xxxtai.express.model.ComGraph"/>

	<bean id="settingGui" class="com.xxxtai.express.view.SettingGui"/>

	<bean id="drawingGui" class="com.xxxtai.express.view.DrawingGui"/>

	<bean id="drawingGraph" class="com.xxxtai.express.view.DrawingGraph"/>

</beans>